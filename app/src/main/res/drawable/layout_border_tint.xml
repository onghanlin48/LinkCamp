<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#FFFFFF" /> <!-- Background color -->
    <stroke
        android:width="2dp"
        android:color="#CBD0DC"
        android:dashWidth="25dp"
        android:dashGap="10dp" />
    <corners android:radius="20dp" /> <!-- Rounded corners (optional) -->
    <padding
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp" /> <!-- Padding inside the border -->
</shape>
